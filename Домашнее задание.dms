   "source": [
    "rating_frame = pd.DataFrame(columns=['Ф.И.О','Город', 'Подразделение','Должность','Рейтинг'])\n",
    "for index, worker in data.iterrows():\n",
    "    rating_frame.loc[index] = [worker[1], worker[-1], worker[8], worker[7],how_he_works(worker, max_exp.iloc[index])]"
